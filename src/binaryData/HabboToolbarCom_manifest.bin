            <asset mimeType="text/xml" name="purse_indicator_shells_xml" />

            <asset mimeType="image/png" name="coin_icon_png" />
            <asset mimeType="image/png" name="pixel_icon_png" />
            <asset mimeType="image/png" name="shell_icon_png" />

            <!-- Club Status Bar -->
            <asset mimeType="text/xml" name="club_bar_xml" />

            <asset mimeType="image/png" name="extend_hilite_png" />

		</assets>
		
	</component>
	
</manifest>
